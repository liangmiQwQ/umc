{
  "oxc.enable": true,
  "oxc.typeAware": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "dprint.dprint",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "rust-analyzer.procMacro.ignored": {
    "napi-derive": ["napi"],
    "tracing": ["instrument"]
  },
  "git.branchProtectionPrompt": "alwaysCommit"
}
